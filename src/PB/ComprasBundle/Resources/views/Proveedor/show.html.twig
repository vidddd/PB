{% extends 'JordiLlonchCrudGeneratorBundle::layout.html.twig' %}

{% block title %}
{{ parent() }} - Proveedor {{ 'views.show.show'|trans({}, 'JordiLlonchCrudGeneratorBundle') }}
{% endblock %}

{% block page %}

<h1>Proveedor</h1>

<form class="form-horizontal">
    <fieldset>        <div class="control-group">
            <label class="control-label">Id</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.id }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Nombre</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.nombre }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Nombrecomercial</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.nombrecomercial }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Nif</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.nif }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Nombrecontacto</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.nombrecontacto }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Telefono_contacto</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.telefonocontacto }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Email_contacto</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.emailcontacto }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Telefono</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.telefono }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Fax</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.fax }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Email</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.email }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Direccion</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.direccion }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Cp</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.cp }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Localidad</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.localidad }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Provincia</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.provincia }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Pais</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.pais }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Tipo_proveedor</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.tipoproveedor }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Es_cliente</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.escliente }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Activo</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.activo }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Observaciones</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.observaciones }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Medio_envio</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.medioenvio }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Paga_iva</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.pagaiva }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Irpf</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.irpf }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Codigo_formapago</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.codigoformapago }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Codigo_externo</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.codigoexterno }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Cuenta_bancaria</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.cuentabancaria }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Codigo_contabilidad</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.codigocontabilidad }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Fechaalta</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.fechaalta|date('Y-m-d H:i:s') }}</span>            </div>
        </div>    </fieldset>
</form>

<div>
    <div class="float-left">
        <a class="btn" href="{{ path('proveedor') }}">
            {{ 'views.recordactions.backtothelist'|trans({}, 'JordiLlonchCrudGeneratorBundle') }}
        </a>
    </div>
    <div class="float-left">
        <a class="btn" href="{{ path('proveedor_edit', { 'id': entity.id }) }}">
            {{ 'views.recordactions.edit'|trans({}, 'JordiLlonchCrudGeneratorBundle') }}
        </a>
    </div>
    <div class="float-left">
        <form action="{{ path('proveedor_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button class="btn btn-danger" type="submit">{{ 'views.recordactions.delete'|trans({}, 'JordiLlonchCrudGeneratorBundle') }}</button>
        </form>
    </div>
</div>

{% endblock %}