            <td>{{ entity.irpf }}</td>
        </tr>
        <tr>
            <th>Codigo_formapago</th>
            <td>{{ entity.codigoformapago }}</td>
        </tr>
        <tr>
            <th>Codigo_externo</th>
            <td>{{ entity.codigoexterno }}</td>
        </tr>
        <tr>
            <th>Cuenta_bancaria</th>
            <td>{{ entity.cuentabancaria }}</td>
        </tr>
        <tr>
            <th>Codigo_contabilidad</th>
            <td>{{ entity.codigocontabilidad }}</td>
        </tr>
        <tr>
            <th>Fechaalta</th>
            <td>{{ entity.fechaalta|date('Y-m-d H:i:s') }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('proveedor') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('proveedor_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('proveedor_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}