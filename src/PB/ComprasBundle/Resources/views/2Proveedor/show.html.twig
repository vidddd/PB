{% extends '::base.html.twig' %}

{% block content %}
<div class="title title-spacing">
					<h2>Proveedor</h2>
</div>
<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Nombre</th>
            <td>{{ entity.nombre }}</td>
        </tr>
        <tr>
            <th>Nombrecomercial</th>
            <td>{{ entity.nombrecomercial }}</td>
        </tr>
        <tr>
            <th>Nif</th>
            <td>{{ entity.nif }}</td>
        </tr>
        <tr>
            <th>Nombrecontacto</th>
            <td>{{ entity.nombrecontacto }}</td>
        </tr>
        <tr>
            <th>Telefono_contacto</th>
            <td>{{ entity.telefonocontacto }}</td>
        </tr>
        <tr>
            <th>Email_contacto</th>
            <td>{{ entity.emailcontacto }}</td>
        </tr>
        <tr>
            <th>Telefono</th>
            <td>{{ entity.telefono }}</td>
        </tr>
        <tr>
            <th>Fax</th>
            <td>{{ entity.fax }}</td>
        </tr>
        <tr>
            <th>Email</th>
            <td>{{ entity.email }}</td>
        </tr>
        <tr>
            <th>Direccion</th>
            <td>{{ entity.direccion }}</td>
        </tr>
        <tr>
            <th>Cp</th>
            <td>{{ entity.cp }}</td>
        </tr>
        <tr>
            <th>Localidad</th>
            <td>{{ entity.localidad }}</td>
        </tr>
        <tr>
            <th>Provincia</th>
            <td>{{ entity.provincia }}</td>
        </tr>
        <tr>
            <th>Pais</th>
            <td>{{ entity.pais }}</td>
        </tr>
        <tr>
            <th>Tipo_proveedor</th>
            <td>{{ entity.tipoproveedor }}</td>
        </tr>
        <tr>
            <th>Es_cliente</th>
            <td>{{ entity.escliente }}</td>
        </tr>
        <tr>
