{% extends '::layout.html.twig' %}

{% block title entity.cliente %}

{% block content %}

<div class="title title-spacing">
					<h2>Pedidos</h2>
</div>

<form class="form-horizontal">
    <fieldset>        <div class="control-group">
            <label class="control-label">Id</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.id }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Codpedido</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.codpedido }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Estado</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.estado }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Codalbaran</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.codalbaran }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Codfactura</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.codfactura }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Fecha</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.fecha|date('d.m.Y') }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Fecha_entrega</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.fechaentrega|date('d.m.Y') }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Subcliente</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.subcliente }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Cantidad</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.cantidad }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Mtycolor</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.mtycolor }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Tipomaterial</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.tipomaterial }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Ancho</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.ancho }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Galga</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.galga }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Plegado</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.plegado }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Bobinas</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.bobinas }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Metros</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.metros }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Pesoteorico</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.pesoteorico }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Tratado</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.tratado }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Tipotubo</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.tipotubo }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Kilosimp</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.kilosimp }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Cliche</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.cliche }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Medidaimp</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.medidaimp }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Soldadura</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.soldadura }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Impresion</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.impresion }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Colores</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.colores }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Maquina</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.maquina }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Cantidadc</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.cantidadc }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Anchoc</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.anchoc }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Largoc</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.largoc }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Solapa</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.solapa }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Almacen</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.almacen }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Operario</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.operario }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Notasextrusion</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.notasextrusion }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Notasimpresion</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.notasimpresion }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Notascorte</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.notascorte }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Precio</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.precio }}</span>            </div>
        </div>    </fieldset>
</form>

<div>
    <div class="float-left">
        <a class="btn" href="{{ path('pedido') }}">
            {{ 'views.recordactions.backtothelist'|trans({}, 'JordiLlonchCrudGeneratorBundle') }}
        </a>
    </div>
    <div class="float-left">
        <a class="btn btn-primary" href="{{ path('pedido_edit', { 'id': entity.id }) }}">
            {{ 'views.recordactions.edit'|trans({}, 'JordiLlonchCrudGeneratorBundle') }}
        </a>
    </div>
    <div class="float-left">
        <form action="{{ path('pedido_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button class="btn btn-danger" type="submit" onclick="return confirm('Desea eliminar al proyecto del sistema?');">{{ 'views.recordactions.delete'|trans({}, 'JordiLlonchCrudGeneratorBundle') }}</button>
        </form>
    </div>
</div>
{% endblock %}